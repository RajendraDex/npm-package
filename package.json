{
  "name": "rajendra-npm-package",
  "version": "1.0.20",
  "main": "dist/src/api/app.bootstrap.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "start": "node dist/src/api/app.bootstrap",
    "bin": "node dist/bin/index.js create",
    "bin:create": "node dist/bin/cmd/create-boilerplate.js --no --app=your-app",
    "bin:cli": "node cmd/create-boilerplate/index.js",
    "dev": "npm run build && npm run start",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint1": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepare": "npm run build"
  },
  "bin": {
    "entity-form-crafter": "dist/bin/index.js",
    "rajendra-npm-package": "dist/bin/cmd/create-boilerplate.js",
    "rajendra-npm-package-cli": "cmd/create-boilerplate/index.js"
  },
  "_moduleAliases": {
    "@/*": "dist/src/*",
    "@bin": "dist/bin",
    "@cmd": "dist/bin/cmd",
    "@utils": "dist/generator/utils",
    "@writers": "dist/generator/writers",
    "@base": "dist/generator/base",
    "@prompt": "dist/generator/prompt",
    "@project": "dist/generator/project",
    "@template": "dist/generator/template",
    "@observer": "dist/generator/observer",
    "@generator": "dist/generator",
    "@factory": "dist/generator/factory",
    "@builder": "dist/generator/builder"
  },
  "keywords": [
    "project",
    "create",
    "new",
    "simple",
    "command"
  ],
  "author": "Rajendra Rajendra <rajendrap.dexbytes@gmail.com>",
  "license": "ISC",
  "description": "create a new project with a simple command",
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "chalk": "^5.4.1",
    "commander": "^13.0.0",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "fs-extra": "^11.3.0",
    "graphql": "^16.10.0",
    "helmet": "^8.0.0",
    "inquirer": "^12.3.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "ora": "^8.1.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RajendraDex/npm-package.git"
  },
  "bugs": {
    "url": "https://github.com/RajendraDex/npm-package/issues"
  },
  "homepage": "https://github.com/RajendraDex/npm-package#readme"
}
