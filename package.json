{
  "name": "rejendra-npm-package",
  "version": "1.0.03",
  "main": "dist/bin/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/index.js",
    "bin": "node dist/bin/index.local.js",
    "dev": "npm run build && npm run start",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint1": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepare": "npm run build"
  },
  "bin": {
    "rejendra-npm-package": "dist/bin/index.js"
  },
  "_moduleAliases": {
    "@/*": "dist/src/*",
    "@bin": "dist/bin",
    "@utils": "dist/generator/utils",
    "@writers": "dist/generator/writers",
    "@base": "dist/generator/base",
    "@prompt": "dist/generator/prompt",
    "@project": "dist/generator/project",
    "@template": "dist/generator/template",
    "@observer": "dist/generator/observer",
    "@generator": "dist/generator",
    "@factory": "dist/generator/factory",
    "@builder": "dist/generator/builder"
  },
  "keywords": [
    "project",
    "create",
    "new",
    "simple",
    "command"
  ],
  "author": "Rajendra Rajendra <rajendrap.dexbytes@gmail.com>",
  "license": "ISC",
  "description": "create a new project with a simple command",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "inquirer": "^12.3.2",
    "module-alias": "^2.2.3",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RajendraDex/npm-package.git"
  },
  "bugs": {
    "url": "https://github.com/RajendraDex/npm-package/issues"
  },
  "homepage": "https://github.com/RajendraDex/npm-package#readme"
}
